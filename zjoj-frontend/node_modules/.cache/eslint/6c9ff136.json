cts/zjoj/zjoj-frontend/src/access/checkAccess.ts",["175"],"import ACCESS_ENUM from \"@/access/accessEnum\";\n\n/**\n * checkAccess: check if current user has permission or not\n * @param loginUser: current login user\n * @param needAccess: permission need\n * @return boolean\n */\nconst checkAccess = (loginUser: any, needAccess = ACCESS_ENUM.NOT_LOGIN) => {\n  //get permission of current user(no loginUser -> not login)\n  const loginUserAccess = loginUser?.userRole ?? ACCESS_ENUM.NOT_LOGIN;\n  //if don't need any permission\n  if (needAccess === ACCESS_ENUM.NOT_LOGIN) {\n    return true;\n  }\n  //just need user to login\n  if (needAccess === ACCESS_ENUM.USER) {\n    //if user not login\n    if (loginUserAccess === ACCESS_ENUM.NOT_LOGIN) return false;\n  }\n\n  //need admin permission\n  if (needAccess === ACCESS_ENUM.ADMIN) {\n    //if current user not admin\n    if (loginUserAccess !== ACCESS_ENUM.ADMIN) return false;\n  }\n  return true;\n};\n\nexport default checkAccess;\n","/Users/zj/projects/zjoj/zjoj-frontend/src/layouts/BasicLayout.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/index.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/layouts/UserLayout.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/AboutView.vue",[],[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/NoAuthView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/AdminView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/user/UserRegisterView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/user/UserLoginView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/core/ApiError.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/core/OpenAPI.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/core/CancelablePromise.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/PostControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/PostThumbControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generate