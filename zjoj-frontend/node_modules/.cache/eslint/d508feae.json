Access = loginUser?.userRole ?? ACCESS_ENUM.NOT_LOGIN;\n  //if don't need any permission\n  if (needAccess === ACCESS_ENUM.NOT_LOGIN) {\n    return true;\n  }\n  //just need user to login\n  if (needAccess === ACCESS_ENUM.USER) {\n    //if user not login\n    if (loginUserAccess === ACCESS_ENUM.NOT_LOGIN) return false;\n  }\n\n  //need admin permission\n  if (needAccess === ACCESS_ENUM.ADMIN) {\n    //if current user not admin\n    if (loginUserAccess !== ACCESS_ENUM.ADMIN) return false;\n  }\n  return true;\n};\n\nexport default checkAccess;\n","/Users/zj/projects/zjoj/zjoj-frontend/src/layouts/UserLayout.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/core/CancelablePromise.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/core/ApiError.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/core/OpenAPI.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/FileControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/PostControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/PostFavourControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/PostThumbControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/WxMpControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/UserControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/generated/services/QuestionControllerService.ts",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/NoAuthView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/AdminView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/user/UserLoginView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/question/AddQuestionView.vue",["170","171","172","173","174"],"<template>\n  <div id=\"addQuestionView\">\n    <h2>Add Question</h2>\n    <a-form :model=\"form\" label-align=\"left\">\n      <a-form-item field=\"title\" label=\"Title\">\n        <a-input v-model=\"form.title\" placeholder=\"please enter title\" />\n      </a-form-item>\n      <a-form-item field=\"tags\" label=\"Tags\">\n        <a-input-tag\n          v-model=\"form.tags\"\n          placeholder=\"please pick labels\"\n          allow-clear\n        />\n      </a-form-item>\n      <a-form-item field=\"content\" label=\"Content\">\n        <MdEditor :value=\"form.content\" :handle-change=\"onContentChange\" />\n      </a-form-item>\n      <a-form-item field=\"answer\" label=\"Answer\">\n        <MdEditor :value=\"form.answer\" :handle-change=\"onAnswerChange\" />\n      </a-form-item>\n      <a-form-item\n        label=\"judgeConfiguration\"\n        :content-flex=\"false\"\n        :merge-props=\"false\"\n      >\n        <a-space direction=\"vertical\" style=\"min-width: 480px\">\n          <a-form-item field=\"judgeConfig.timeLimit\" label=\"TimeLimit\">\n            <a-input-number\n              v-model=\"form.judgeConfig.timeLimit\"\n              placeholder=\"please enter time limit\"\n              mode=\"button\"\n              min=\"0\"\n              size=\"large\"\n            />\n          </a-form-item>\n          <a-form-item field=\"judgeConfig.memoryLimit\" label=\"MemoryLimit\">\n            <a-input-number\n              v-model=\"form.judgeConfig.memoryLimit\"\n              placeholder=\"please enter memory limit\"\n              mode=\"button\"\n              min=\"0\"\n              size=\"large\"\n            />\n          </a-form-item>\n          <a-form-item field=\"judgeConfig.stackLimit\" label=\"StackLimit\">\n            <a-input-number\n              v-model=\"form.judgeConfig.stackLimit\"\n              placeholder=\"please enter stack limit\"\n              mode=\"button\"\n              min=\"0\"\n              size=\"large\"\n            />\n          </a-form-item>\n        </a-space>\n      </a-form-item>\n      <a-form-item label=\"judgeCase\" :content-flex=\"false\" :merge-props=\"false\">\n        <a-form-item\n          v-for=\"(judgeCaseItem, index) of form.judgeCase\"\n          :key=\"index\"\n          no-style\n        >\n          <a-space direction=\"vertical\" style=\"min-width: 640px\">\n            <a-form-item\n              :field=\"`form.judgeCase[${index}].input`\"\n              :label=\"`inputCase-${index}`\"\n              :key=\"index\"\n            >\n              <a-input\n                v-model=\"judgeCaseItem.input\"\n                placeholder=\"please enter user input case\"\n              />\n            </a-form-item>\n            <a-form-item\n              :field=\"`form.judgeCase[${index}].output`\"\n              :label=\"`outputCase-${index}`\"\n              :key=\"index\"\n            >\n              <a-input\n                v-model=\"judgeCaseItem.output\"\n                placeholder=\"please enter user output case\"\n              />\n            </a-form-item>\n            <a-button status=\"danger\" @click=\"handleDelete(index)\">\n              Delete\n            </a-button>\n          </a-space>\n        </a-form-item>\n        <div style=\"margin-top: 32px\">\n          <a-button @click=\"handleAdd\" type=\"outline\" status=\"success\"\n            >Add Judge Case\n          </a-button>\n        </div>\n      </a-form-item>\n      <div style=\"margin-top: 16px\" />\n      <a-form-item>\n        <a-button type=\"primary\" style=\"min-width: 200px\" @click=\"doSubmit\"\n          >Submit\n        </a-button>\n      </a-form-item>\n    </a-form>\n  </div>\n</template>\n\n<script setup lang=\"ts\">\nimport { onMounted, ref } from \"vue\";\nimport MdEditor from \"@/components/MdEditor.vue\";\nimport { QuestionControllerService } from \"../../../generated\";\nimport message from \"@arco-design/web-vue/es/message\";\nimport { useRoute } from \"vue-router\";\n\nconst route = useRoute();\n//if url include \"update\" -> update\nconst updatePage = route.path.includes(\"update\");\n\nlet form = ref({\n  title: \"\",\n  tags: [],\n  answer: \"\",\n  content: \"\",\n  judgeConfig: {\n    timeLimit: 1000,\n    memoryLimit: 1000,\n    stackLimit: 1000,\n  },\n  judgeCase: [\n    {\n      input: \"\",\n      output: \"\",\n    },\n  ],\n});\n\n/**\n * use id to get old data\n */\nconst loadData = async () => {\n  const id = route.query.id;\n  if (!id) {\n    return;\n  }\n  const res = await QuestionControllerService.getQuestionByIdUsingGet(\n    id as any\n  );\n  if (res.code === 0) {\n    form.value = res.data as any;\n    //JSON -> String\n    if (!form.value.judgeCase) {\n      form.value.judgeCase = [\n        {\n          input: \"\",\n          output: \"\",\n        },\n      ];\n    } else {\n      form.value.judgeCase = JSON.parse(form.value.judgeCase as any);\n    }\n    if (!form.value.judgeConfig) {\n      form.value.judgeConfig = {\n        timeLimit: 1000,\n        memoryLimit: 1000,\n        stackLimit: 1000,\n      };\n    } else {\n      form.value.judgeConfig = JSON.parse(form.value.judgeConfig as any);\n    }\n    if (!form.value.tags) {\n      form.value.tags = [];\n    } else {\n      form.value.tags = JSON.parse(form.value.tags as any);\n    }\n  } else {\n    message.error(\"Load Fail\" + res.message);\n  }\n};\n\nonMounted(() => {\n  loadData();\n});\n\nconst onContentChange = (value: string) => {\n  form.value.content = value;\n};\n\nconst onAnswerChange = (value: string) => {\n  form.value.answer = value;\n};\n\n/**\n * Add judgeCase\n */\nconst handleAdd = () => {\n  form.value.judgeCase.push({\n    input: \"\",\n    output: \"\",\n  });\n};\n\n/**\n * Delete judgeCase\n * @param index\n */\nconst handleDelete = (index: number) => {\n  form.value.judgeCase.splice(index, 1);\n};\n\n/**\n * submit form\n */\nconst doSubmit = async () => {\n  //console.log(form.value);\n  //Distinguish between update and add\n  if (updatePage) {\n    const res = await QuestionControllerService.updateQuestionUsingPost(\n      form.value\n    );\n    if (res.code === 0) {\n      message.success(\"update question success\");\n    } else {\n      message.error(\"update question fail, \" + res.message);\n    }\n  } else {\n    const res = await QuestionControllerService.addQuestionUsingPost(\n      form.value\n    );\n    if (res.code === 0) {\n      message.success(\"add question success\");\n    } else {\n      message.error(\"add question fail, \" + res.message);\n    }\n  }\n};\n</script>\n\n<style scoped>\n#addQuestionView {\n}\n</style>\n","/Users/zj/projects/zjoj/zjoj-frontend/src/views/user/UserRegisterView.vue",[],"/Users/zj/projects/zjoj/zjoj-frontend/src/views/question/QuestionsView.vue",["175"],"<template>\n  <div id=\"QuestionsView\">\n    <h2>Questions View</h2>\n    <a-form :model=\"searchParams\" layout=\"inline\" style=\"min-width: 240px\">\n      <a-form-item field=\"title\" label=\"Title\">\n        <a-input\n          v-model=\"searchParams.title\"\n          placeholder=\"please enter question title\"\n        />\n      </a-form-item>\n      <a-form-item field=\"tags\" label=\"Tags\" style=\"min-width: 240px\">\n        <a-input-tag\n          v-model=\"searchParams.tags\"\n          placeholder=\"please enter question tags\"\n        />\n      </a-form-item>\n      <a-form-item>\n        <a-button type=\"primary\" @click=\"doSubmit\">Submit</a-button>\n      </a-form-item>\n    </a-form>\n    <a-divider size=\"0\" />\n    <a-table\n      :ref=\"tableRef\"\n      :columns=\"columns\"\n      :data=\"dataList\"\n      :pagination=\"{\n        showTotal: true,\n        pageSize: searchParams.pageSize,\n        current: searchParams.current,\n        total,\n      }\"\n      @page-change=\"onPageChange\"\n    >\n      <template #tags=\"{ record }\">\n        <a-space wrap>\n          <a-tag v-for=\"(tag, index) of record.tags\" :key=\"index\" color=\"green\"\n            >{{ tag }}\n          </a-tag>\n        </a-space>\n      </template>\n      <template #acceptedRate=\"{ record }\">\n        {{\n          `${record.submitNum ? record.acceptedNum / record.sub